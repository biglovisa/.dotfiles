###	Aliases

###	Git

alias glg="git log --graph --decorate --oneline --abbrev-commit"
alias gp="git push origin HEAD"
alias gd="git diff"
alias gdc="git diff --cached"
alias gc="git commit"
alias gco="git checkout"
alias gb="git branch"
alias ga="git add"
alias gs="git status -sb"
alias gcl="git clone"
alias grv="git remote -v"

alias hub="git hub"

###	Shell

alias ll="ls -la"
alias cl="clear"
alias dots="cd ~/.dotfiles"
alias path='echo $PATH | tr -s ":" "\n"'
alias st="subl"

### Applications

alias mk="minikube"
alias kctl="kubectl"

alias pe="pyenv"

###	Change directories

alias cod="cd ~/Code"
